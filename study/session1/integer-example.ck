// Integer examples

// define integers either 
int a;

// or store directly into the definition
2 => int b;

// print b
<<< b >>>;

// store value directly into c
3 => int c;

// print c
<<< c >>>;

// multiply
10 * b => int d;

// print d showing result of multiply
<<< d >>>;

// shorter multiply example by itself
10 *=> d;

// print new value for d
<<< d >>>;
